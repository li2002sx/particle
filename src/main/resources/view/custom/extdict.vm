<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Analyzer</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Basic Form Elements
                </div>
                <div class="panel-body">
                    <div class="row ">
                        <div class="col-lg-12">
                            <form id="term_form" method="post">
                                <div class="form-group">
                                    <label>扩展词汇（一行一个）</label>
                                    <textarea class="form-control" name="terms" rows="8">$!terms</textarea>
                                </div>

                                <button type="button" class="btn btn-primary" id="btn_add_extdic">Submit Button</button>
                                <button type="button" class="btn btn-info" id="btn_reset_extdic">Reset Button</button>
                            </form>
                        </div>
                    </div>
                    <!-- /.row (nested) -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <div class="col-lg-12">
            <div class="">
                <div class="panel panel-red">
                    <div class="panel-heading">
                        Red Panel
                    </div>
                    <div class="panel-body">
                        <p>$!data</p>
                    </div>
                    <div class="panel-footer">
                        Panel Footer
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- /.row -->
</div>
<!-- /#page-wrapper -->

<!-- Custom JS -->
#set($jsUrls=["js/custom/extdict.js"])